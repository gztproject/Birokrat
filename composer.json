{
	"name" : "gzt/birokrat",
	"type" : "project",
	"license" : "MIT",
	"description" : "A DIY web-based accounting software",
	"require" : {
		"php": "^8.0",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"doctrine/annotations": "^1.0",
		"doctrine/doctrine-bundle": "^2.5",
		"doctrine/doctrine-migrations-bundle": "^3.2",
		"doctrine/orm": "~2.8",
		"erusev/parsedown": "~1.7",
		"ezyang/htmlpurifier": "~4.13",
		"friendsofsymfony/jsrouting-bundle": "~3.1",
		"knplabs/knp-paginator-bundle": "~5.4",
		"nelmio/cors-bundle": "~2.1",
		"phpdocumentor/reflection-docblock": "^5.3",
		"phpoffice/phpspreadsheet": "~1.17",
		"phpstan/phpdoc-parser": "^1.2",
		"qipsius/tcpdf-bundle": "~2.0",
		"ramsey/uuid": "~4.1",
		"ramsey/uuid-doctrine": "~1.6",
		"sensio/framework-extra-bundle": "~6.1",
		"symfony/apache-pack": "~1.0",
		"symfony/asset": "~6.0",
		"symfony/console": "~6.0",
		"symfony/dotenv": "~6.0",
		"symfony/expression-language": "~6.0",
		"symfony/flex": "~1.12",
		"symfony/form": "~6.0",
		"symfony/framework-bundle": "~6.0",
		"symfony/http-client": "~6.0",
		"symfony/mailer": "~6.0",
		"symfony/mailgun-mailer": "~6.0",
		"symfony/maker-bundle": "~1.30",
		"symfony/monolog-bundle": "~3.7",
		"symfony/process": "~6.0",
		"symfony/property-access": "~6.0",
		"symfony/property-info": "~6.0",
		"symfony/proxy-manager-bridge": "~6.0",
		"symfony/security-bundle": "~6.0",
		"symfony/serializer": "~6.0",
		"symfony/translation": "~6.0",
		"symfony/twig-bundle": "~6.0",
		"symfony/validator": "~6.0",
		"symfony/web-link": "~6.0",
		"symfony/webpack-encore-bundle": "~1.11",
		"symfony/yaml": "~6.0",
		"twig/extra-bundle": "^3.3",
		"twig/intl-extra": "^3.3"
	},
	"require-dev" : {
		"doctrine/doctrine-fixtures-bundle": "~3.4",
		"phpunit/phpunit": "^9.5",
		"symfony/browser-kit": "~6.0",
		"symfony/css-selector": "~6.0",
		"symfony/debug-bundle": "~6.0",
		"symfony/phpunit-bridge": "~6.0",
		"symfony/stopwatch": "~6.0",
		"symfony/web-profiler-bundle": "~6.0"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true,
		"allow-plugins" : {
			"symfony/flex" : true
		}
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"paragonie/random_compat" : "2.*",
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "~6.0"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false,
			"require" : "~6.0"
		}
	},
	"support" : {
		"email" : "gasper.doljak@gmail.com",
		"issues" : "https://github.com/gztproject/Birokrat/issues",
		"wiki" : "https://github.com/gztproject/Birokrat/wiki"
	},
	"minimum-stability" : "stable",
	"authors" : [{
			"name" : "Ga≈°per Doljak",
			"email" : "gasper.doljak@gmail.com",
			"homepage" : "https://www.gzt.si",
			"role" : "Main dev"
		}
	],
	"scripts": {
		"auto-scripts": {

		}
	}
}
