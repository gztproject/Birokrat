{
	"name" : "gzt/birokrat",
	"type" : "project",
	"license" : "MIT",
	"description" : "A DIY web-based accounting software",
	"require" : {
		"php" : "^7.4",
		"ext-ctype" : "*",
		"ext-iconv" : "*",
		"doctrine/orm" : "~2.7",
		"erusev/parsedown" : "~1.7",
		"ezyang/htmlpurifier" : "~4.12",
		"friendsofsymfony/jsrouting-bundle" : "~2.5",
		"knplabs/knp-paginator-bundle" : "~5.0",
		"nelmio/cors-bundle" : "~2.0",
		"qipsius/tcpdf-bundle" : "^2.0",
		"ramsey/uuid" : "~3.9",
		"ramsey/uuid-doctrine" : "~1.5",
		"sensio/framework-extra-bundle" : "~5.5",
		"sensiolabs/security-checker" : "~6.0",
		"symfony/apache-pack" : "~1.0",
		"symfony/asset" : "~5.0",
		"symfony/console" : "~5.0",
		"symfony/dotenv" : "~5.0",
		"symfony/expression-language" : "~5.0",
		"symfony/flex" : "~1.5",
		"symfony/form" : "~5.0",
		"symfony/framework-bundle" : "~5.0",
		"symfony/http-client" : "~5.0",
		"symfony/mailer" : "~5.0",
		"symfony/mailgun-mailer" : "~5.0",
		"symfony/maker-bundle" : "~1.14",
		"symfony/monolog-bundle" : "~3.5",
		"symfony/orm-pack" : "~1.0",
		"symfony/process" : "~5.0",
		"symfony/security-bundle" : "~5.0",
		"symfony/serializer-pack" : "~1.0",
		"symfony/translation" : "~5.0",
		"symfony/twig-bundle" : "5.0.*",
		"symfony/validator" : "~5.0",
		"symfony/web-link" : "~5.0",
		"symfony/webpack-encore-bundle" : "~1.7",
		"symfony/yaml" : "~5.0",
		"twig/extensions" : "~1.5"
	},
	"require-dev" : {
		"doctrine/doctrine-fixtures-bundle" : "~3.3",
		"symfony/browser-kit" : "~5.0",
		"symfony/css-selector" : "~5.0",
		"symfony/debug-bundle" : "~5.0",
		"symfony/debug-pack" : "~1.0",
		"symfony/phpunit-bridge" : "~5.0",
		"symfony/profiler-pack" : "~1.0",
		"symfony/stopwatch" : "~5.0",
		"symfony/test-pack" : "~1.0",
		"symfony/web-profiler-bundle" : "~5.0"
	},
	"config" : {
		"preferred-install" : {
			"*" : "dist"
		},
		"sort-packages" : true
	},
	"autoload" : {
		"psr-4" : {
			"App\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"App\\Tests\\" : "tests/"
		}
	},
	"replace" : {
		"paragonie/random_compat" : "2.*",
		"symfony/polyfill-ctype" : "*",
		"symfony/polyfill-iconv" : "*",
		"symfony/polyfill-php71" : "*",
		"symfony/polyfill-php70" : "*",
		"symfony/polyfill-php56" : "*"
	},
	"conflict" : {
		"symfony/symfony" : "5.0.*"
	},
	"extra" : {
		"symfony" : {
			"allow-contrib" : false,
			"require" : "5.0.*"
		}
	},
	"support" : {
		"email" : "gasper.doljak@gmail.com",
		"issues" : "https://github.com/gztproject/Birokrat/issues",
		"wiki" : "https://github.com/gztproject/Birokrat/wiki"
	},
	"minimum-stability" : "stable",
	"authors" : [{
			"name" : "Ga≈°per Doljak",
			"email" : "gasper.doljak@gmail.com",
			"homepage" : "https://www.gzt.si",
			"role" : "Main dev"
		}
	]
}